{
  "name": "hermione",
  "version": "0.52.1",
  "description": "Tests framework based on mocha and wdio",
  "main": "lib/hermione.js",
  "scripts": {
    "lint": "eslint .",
    "test-unit": "istanbul test _mocha -- test/",
    "test": "npm run lint && npm run test-unit",
    "toc": "doctoc README.md",
    "precommit": "npm run lint",
    "preversion": "npm run lint && npm test",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/gemini-testing/hermione.git"
  },
  "engines": {
    "node": ">= 6.4.0"
  },
  "keywords": [
    "hermione",
    "mocha",
    "test",
    "webdriverio"
  ],
  "bin": {
    "hermione": "./bin/hermione"
  },
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "bluebird-q": "^2.1.1",
    "chalk": "^1.1.1",
    "clear-require": "^1.0.1",
    "commander": "^2.9.0",
    "gemini-configparser": "^1.0.0",
    "gemini-core": "^2.1.0",
    "glob-extra": "^2.0.0",
    "inherit": "^2.2.2",
    "lodash": "^4.17.4",
    "looks-same": "^3.2.1",
    "mocha": "~2.4.5",
    "plugins-loader": "^1.1.0",
    "png-img": "^2.1.1",
    "q": "^2.0.0",
    "q-promise-utils": "^1.0.0",
    "teamcity-service-messages": "^0.1.6",
    "temp": "^0.8.3",
    "urijs": "^1.17.0",
    "webdriverio": "gemini-testing/webdriverio#master",
    "worker-farm": "git://github.com/gemini-testing/node-worker-farm.git#release/feat/broadcast"
  },
  "devDependencies": {
    "app-module-path": "^2.2.0",
    "bluebird": "^3.5.1",
    "chai": "^4.1.1",
    "chai-as-promised": "^7.1.1",
    "conventional-changelog-lint": "^1.0.1",
    "doctoc": "^1.0.0",
    "eslint": "^3.9.0",
    "eslint-config-gemini-testing": "^2.4.0",
    "husky": "^0.11.4",
    "istanbul": "^0.4.1",
    "proxyquire": "^1.7.3",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.12.0",
    "standard-version": "^3.0.0"
  }
}
